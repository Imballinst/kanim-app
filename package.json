{
  "name": "kanim",
  "version": "1.0.0",
  "description": "aplikasi untuk notifikasi antrian kanim",
  "main": "index.js",
  "scripts": {
    "start": "pm2 start ecosystem.config.js",
    "restart": "pm2 restart kanim-app",
    "stop": "pm2 stop kanim-app --watch",
    "delete": "pm2 delete kanim-app",
    "log": "pm2 logs kanim-app",
    "show": "pm2 show kanim-app",
    "test": "jest && eslint --fix ./index.js ./lib/."
  },
  "author": "Try Ajitiono",
  "license": "MIT",
  "keywords": [
    "kanim",
    "notifikasi"
  ],
  "jest": {
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js)$",
    "moduleFileExtensions": [
      "js"
    ],
    "setupFiles": [
      "./scripts/JestSetup.js"
    ]
  },
  "pre-commit": [
    "test"
  ],
  "dependencies": {
    "@sendgrid/mail": "^6.2.1",
    "axios": "^0.17.1",
    "bluebird": "^3.5.1",
    "cron": "^1.3.0",
    "date-fns": "^2.0.0-alpha.7",
    "dotenv": "^4.0.0",
    "lodash": "^4.17.5",
    "mongodb": "^3.0.4",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^22.0.4",
    "pre-commit": "^1.2.2"
  }
}
